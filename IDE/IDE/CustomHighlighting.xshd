		</Keywords>
		
		<Keywords fontWeight="bold" foreground="#B22222">	<!--ADDITIONAL COMMANDS-->
			<Word>,</Word>
			<Word>'</Word>
		</Keywords>
		
		
		<!-- Digits -->
		<Rule foreground="#D1D1D1">
            \b0[xX][0-9a-fA-F]+  # hex number
        |    \b
            (    \d+(\.[0-9]+)?   #number with optional floating point
            |    \.[0-9]+         #or just starting with floating point
            )
            ([eE][+-]?[0-9]+)? # optional exponent
        </Rule>
	</RuleSet>
</SyntaxDefinition>
